<form method="POST" action="adminAddLocation">
    <label for="lName">Name:</label>
    <input type="text" id="lName" name="locationName"/>
    <label for="lAddress">Address:</label>
    <input type="text" id="lAddress" name="address"/>
    <label for="lCity">City:</label>
    <input type="text" id="lCity" name="city"/>
    <label for="lState">State:</label>
    <input type="text" id="lState" name="state"/>
    <label for="lZip">Zip:</label>
    <input type="text" id="lZip" name="zip"/>
    <label for="lDescription">Description:</label>
    <input type="textarea" id="lDescription" name="description"/>
    <label for="lType">Type:</label>
    <input type="text" id="lType" name="type"/>

    <br/>
    <button type="submit">Submit</button>
</form>
